<h1>Your Music Libraries</h1>

<table>
<% @libraries.each do |library| %>
  <tr>
    <td><%= link_to library.name, library_path(library) %></td>
    <td>
      <%= link_to 'Edit', edit_library_path(library) %>
      <%= link_to 'Delete', library_path(library), method: :delete, data: {confirm: 'Are you sure?'} %>
    </td>
  </tr>
<% end %>
</table>
<br>
<%= link_to "New library", new_library_path %>